    "np": "^10.0.7",
    "prettier": "^2.8.8",
    "react-native-svg-transformer": "^1.5.0",
    "react-test-renderer": "^18.3.1",
    "tailwindcss": "3.3.2",
    "ts-jest": "^29.2.4",
    "typescript": "~5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user/repo-name.git"
  },
  "packageManager": "pnpm@8.6.5",
