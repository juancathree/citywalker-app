    "@shopify/flash-list": "1.6.4",
    "@shopify/react-native-skia": "^1.3.10",
    "@tanstack/react-query": "^5.51.21",
    "@ui-kitten/components": "^5.3.1",
    "app-icon-badge": "^0.0.15",
    "axios": "^1.7.3",
    "expo": "~51.0.24",
